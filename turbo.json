{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**"]
        },
        "lint": {},
        "check": {},
        "typecheck": {},
        "cleanup": { "cache": false },
        "start": { "cache": false },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "storybook": {
            "cache": false,
            "persistent": true
        },
        "docker:build": {},
        "docker:push": {},
        "docker:run": {},
        "docker:shell": {},
        "docker:image": {}
    }
}
