{
    "name": "docs",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "VITE_CJS_IGNORE_WARNING=true storybook dev -p 6006 --no-open",
        "build": "VITE_CJS_IGNORE_WARNING=true storybook build -o storybook-static",
        "start": "pnpm dlx serve storybook-static -l 6006 -n -C",
        "dev:app": "vite --clearScreen false",
        "build:app": "tsc && vite build",
        "start:app": "pnpm dlx serve dist -l 4200 -s -n -C",
        "lint": "biome lint . --write",
        "check": "biome check . --write",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@repo/ui-react": "workspace:*",
        "consola": "^3.4.0",
        "lucide-react": "^0.474.0",
        "pathe": "^2.0.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router": "^7.1.3",
        "std-env": "^3.8.0"
    },
    "devDependencies": {
        "@biomejs/biome": "catalog:",
        "@repo/config-typescript": "workspace:*",
        "@repo/core-ui": "workspace:*",
        "@storybook/addon-essentials": "^8.5.1",
        "@storybook/addon-links": "^8.5.1",
        "@storybook/addon-themes": "^8.5.1",
        "@storybook/blocks": "^8.5.1",
        "@storybook/manager-api": "^8.5.1",
        "@storybook/react": "^8.5.1",
        "@storybook/react-vite": "^8.5.1",
        "@storybook/test": "^8.5.1",
        "@storybook/theming": "^8.5.1",
        "@tailwindcss/vite": "^4.0.0",
        "@types/node": "^22.10.10",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@vitejs/plugin-react": "^4.3.4",
        "storybook": "^8.5.2",
        "tailwindcss": "^4.0.0",
        "tailwindcss-motion": "^1.0.1",
        "typescript": "catalog:",
        "vite": "^6.0.11",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
